{
  "get": {
    "operationId": "getExtendedEventInfo",
    "summary": "Get full information about a single event, including day schedule and clock information. (Requires authentication for the event.)",
    "parameters": [
      {"$ref": "../components/eventId.json"}
    ],
    "security": [
      {"sessionTokenAuth": []}
    ],
    "responses": {
      "200": {
        "description": "Returns the extended event data.",
        "content": {
          "application/json": {
            "schema": {"$ref": "../schema/ExtendedEvent.json"}
          }
        }
      },
      "403": {"$ref": "../components/authorizationError.json"},
      "404": {
        "description": "The requested event does not exist.",
        "content": {
          "application/json": {
            "schema": {"$ref": "../schema/Error.json"}
          }
        }
      },
      "500": {"$ref": "../components/serverError.json"}
    }
  }
}
